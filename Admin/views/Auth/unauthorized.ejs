<!doctype html>
<html lang="en">

<head>

    <meta charset="utf-8" />
    <meta name="robots" content="noindex">
    <title>Log In | <%=sitename%>
    </title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta content="game" name="description" />
    <meta content="<%=sitename%>" name="author" />
    <!-- App favicon -->
    <link rel="shortcut icon" href="/public/assets/images/favicon.ico">

    <%- include ('../Partials/HeaderStyle') %>

</head>

<body class="authentication-bg">

    <!-- <div class="home-btn d-none d-sm-block">
        <a href="/" class="text-dark"><i class="mdi mdi-home-variant h2"></i></a>
    </div> -->
    <div class="account-pages my-5 pt-sm-5">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <div class="text-center">
                        <a href="<%=process.env.ADMIN_URL%>/" class=" d-block auth-logo">
                            <img src="<%=siteLogoUrl%>" alt="" height="100" class="logo logo-dark">
                            <img src="<%=siteLogoUrl%>" alt="" height="100" class="logo logo-light">
                        </a>
                    </div>
                </div>
            </div>
            <div class="row align-items-center justify-content-center">
                <div class="col-md-8 col-lg-6 col-xl-5">
                    <div class="card">
                        <div class="card-body p-4">
                            <div class="text-center mt-2">
                                <h5 class="text-primary">Welcome Back !</h5>
                                <p class="text-muted">Sign in to continue to <%=sitename%>
                                </p>
                            </div>
                            <div class="p-2 mt-4">
                                <% if(error.length> 0) { %>
                                    <div class="alert alert-danger text-center mb-4 flash" role="alert">
                                        <%= error %>
                                    </div>
                                    <% } %>
                                        <% if(message.length> 0) { %>
                                            <div class="alert alert-success text-center mb-4 flash" role="alert">
                                                <%= message %>
                                            </div>
                                            <% } %>

                                            <h1>Unauthorized Access</h1>
                                            <p>You do not have permission to view this page. Please <a href="/login">login</a> to continue.</p>
                                        
                            </div>
                        </div>
                    </div>

                    <div class="mt-5 text-center">
                        <p>Â©
                            <script>document.write(new Date().getFullYear())</script> Panel+ by <%=sitename%>.
                        </p>
                    </div>
                </div>
            </div>
            <!-- end row -->
        </div>
        <!-- end container -->
    </div>

    <%- include ('../Partials/VendorScript') %>

        <script src="/public/assets/js/app.js"></script>

</body>

</html>