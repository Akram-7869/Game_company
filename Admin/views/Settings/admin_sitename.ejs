<%- contentFor('HeaderCss') %>
    <link href="/public/assets/libs/sweetalert2/sweetalert2.min.css" rel="stylesheet" type="text/css" />
    <%- contentFor('breadcrumb') %>

        <!-- start page title -->
        <div class="row">
            <div class="col-12">
                <div class="page-title-box d-flex align-items-center justify-content-between">
                    <h4 class="mb-0">
                        <%- title %>
                    </h4>

                    <div class="page-title-right">
                        <ol class="breadcrumb m-0">
                            <li class="breadcrumb-item"><a href="javascript: void(0);">Home</a></li>
                            <li class="breadcrumb-item active"><a href="<%=process.env.ADMIN_URL%>/admin/settings">
                                    <%- title %>
                                </a></li>
                            <li class="breadcrumb-item active">Edit</li>
                        </ol>
                    </div>

                </div>
            </div>
        </div>
        <!-- end page title -->

        <%- contentFor('body') %>


            <div class="row">
                <div class="col-xl-12">
                    <div class="card">
                        <div class="card-body">
                            <% if(error.length> 0) { %>
                                <div class="alert alert-danger text-center mb-4 flash" role="alert">
                                    <%= error %>
                                </div>
                                <% } %>
                                    <% if(message.length> 0) { %>
                                        <div class="alert alert-success text-center mb-4 flash" role="alert">
                                            <%= message %>
                                        </div>
                                        <% } %>
                                            <ul class="nav nav-tabs nav-tabs-custom nav-justified" role="tablist">
                                                <li class="nav-item">
                                                    <a class="nav-link active" data-bs-toggle="tab" href="#sitename"
                                                        role="tab">
                                                        <span class="d-block d-sm-none"><i
                                                                class="fas fa-home"></i></span>
                                                        <span class="d-none d-sm-block">Site Name</span>
                                                    </a>
                                                </li>
                                                <li class="nav-item">
                                                    <a class="nav-link" data-bs-toggle="tab" href="#logo" role="tab">
                                                        <span class="d-block d-sm-none"><i
                                                                class="far fa-user"></i></span>
                                                        <span class="d-none d-sm-block">Site Logo</span>
                                                    </a>
                                                </li>
                                                <li class="nav-item">
                                                    <a class="nav-link" data-bs-toggle="tab" href="#supportemail"
                                                        role="tab">
                                                        <span class="d-block d-sm-none"><i
                                                                class="far fa-envelope"></i></span>
                                                        <span class="d-none d-sm-block">Support Email</span>
                                                    </a>
                                                </li>
                                                <li class="nav-item">
                                                    <a class="nav-link" data-bs-toggle="tab" href="#firebase"
                                                        role="tab">
                                                        <span class="d-block d-sm-none"><i
                                                                class="fas fa-cog"></i></span>
                                                        <span class="d-none d-sm-block">Firebase</span>
                                                    </a>
                                                </li>
                                                <li class="nav-item">
                                                    <a class="nav-link" data-bs-toggle="tab" href="#step" role="tab">
                                                        <span class="d-block d-sm-none"><i
                                                                class="fas fa-cog"></i></span>
                                                        <span class="d-none d-sm-block">2 step auth</span>
                                                    </a>
                                                </li>
                                            </ul>

                                            <!-- Tab panes -->
                                            <div class="tab-content p-3 text-muted">
                                                <div class="tab-pane active" id="stiename" role="tabpanel">
                                                    <p class="mb-0">
                                                    <form class="custom-validation" action=""
                                                        onsubmit="handleSubmit(this)" method="POST" name="player"
                                                        enctype="application/x-www-form-urlencoded">



                                                        <div class="mb-3">
                                                            <label class="form-label">Site name</label>
                                                            <input type="text" class="form-control" required
                                                                name="one[site_name]"
                                                                value="<%= row ? row.one['site_name']:''%>"
                                                                placeholder="" />
                                                        </div>

                                                        <div>
                                                            <div>
                                                                <button type="submit"
                                                                    class="btn btn-primary waves-effect waves-light me-1">
                                                                    Submit
                                                                </button>
                                                                <button type="reset"
                                                                    class="btn btn-secondary waves-effect">
                                                                    Cancel
                                                                </button>
                                                            </div>
                                                        </div>
                                                    </form>
                                                    </p>
                                                </div>
                                                <div class="tab-pane" id="logo" role="tabpanel">
                                                    <p class="mb-0">
                                                    <form class="custom-validation" action=""
                                                        onsubmit="handleSubmit(this)" method="POST" name="player"
                                                        enctype="application/x-www-form-urlencoded">



                                                        <div class="mb-3">
                                                            <label class="form-label">Site Logo</label>
                                                            <input type="file" class="form-control" required
                                                                name="one[site_name]"
                                                                value="<%= row ? row.one['site_name']:''%>"
                                                                placeholder="" />
                                                        </div>

                                                        <div>
                                                            <div>
                                                                <button type="submit"
                                                                    class="btn btn-primary waves-effect waves-light me-1">
                                                                    Submit
                                                                </button>
                                                                <button type="reset"
                                                                    class="btn btn-secondary waves-effect">
                                                                    Cancel
                                                                </button>
                                                            </div>
                                                        </div>
                                                    </form>

                                                    </p>
                                                </div>
                                                <div class="tab-pane" id="supportemail" role="tabpanel">
                                                    <p class="mb-0">
                                                    <form class="custom-validation" action=""
                                                        onsubmit="handleSubmit(this)" method="POST" name="player"
                                                        enctype="application/x-www-form-urlencoded">
                                                        <div class="mb-3">
                                                            <label class="form-label">Support Email</label>
                                                            <input type="text" class="form-control" required
                                                                name="one[support_email]"
                                                                value="<%= row ? row.one['support_email']:''%>"
                                                                placeholder="" />
                                                        </div>
                                                        <div>
                                                            <div>
                                                                <button type="submit"
                                                                    class="btn btn-primary waves-effect waves-light me-1">
                                                                    Submit
                                                                </button>
                                                                <button type="reset"
                                                                    class="btn btn-secondary waves-effect">
                                                                    Cancel
                                                                </button>
                                                            </div>
                                                        </div>
                                                    </form>
                                                    </p>
                                                </div>
                                                <div class="tab-pane" id="firebase" role="tabpanel">
                                                    <p class="mb-0">
                                                    <form class="custom-validation" action=""
                                                        onsubmit="handleSubmit(this)" method="POST" name="player"
                                                        enctype="application/x-www-form-urlencoded">
                                                        <div class="mb-3">
                                                            <label class="form-label">Firebase key</label>
                                                            <input type="text" class="form-control" required
                                                                name="one[firebase_key]"
                                                                value="<%= row ? row.one['firebase_key']:''%>"
                                                                placeholder="" />
                                                        </div>
                                                        <div>
                                                            <div>
                                                                <button type="submit"
                                                                    class="btn btn-primary waves-effect waves-light me-1">
                                                                    Submit
                                                                </button>
                                                                <button type="reset"
                                                                    class="btn btn-secondary waves-effect">
                                                                    Cancel
                                                                </button>
                                                            </div>
                                                        </div>
                                                    </form>
                                                    </p>
                                                </div>
                                                <div class="tab-pane" id="step" role="tabpanel">
                                                    <p class="mb-0">
                                                    <form class="custom-validation" action=""
                                                        onsubmit="handleSubmit(this)" method="POST" name="player"
                                                        enctype="application/x-www-form-urlencoded">
                                                        <div class="mb-3">
                                                            <label class="form-label">2step auth</label>
                                                            <input type="text" class="form-control" required
                                                                name="one[2step_auth]"
                                                                value="<%= row ? row.one['2step_auth']:''%>"
                                                                placeholder="" />
                                                        </div>
                                                        <div>
                                                            <div>
                                                                <button type="submit"
                                                                    class="btn btn-primary waves-effect waves-light me-1">
                                                                    Submit
                                                                </button>
                                                                <button type="reset"
                                                                    class="btn btn-secondary waves-effect">
                                                                    Cancel
                                                                </button>
                                                            </div>
                                                        </div>
                                                    </form>
                                                    </p>
                                                </div>
                                            </div>

                        </div>
                    </div>
                </div> <!-- end col -->


            </div> <!-- end row -->

            <%- contentFor('FooterJs') %>

                <!-- parsleyjs -->
                <script src="/public/assets/libs/parsleyjs/parsley.min.js"></script>

                <script src="/public/assets/js/pages/form-validation.init.js"></script>
                <!-- Sweet Alerts js -->
                <script src="/public/assets/libs/sweetalert2/sweetalert2.min.js"></script>
                <script>
                    function handleSubmit(params) {
                        if (params.checkValidity()) {
                            //Swal.showLoading();
                            Swal.fire({
                                title: '',
                                html: 'Please wait...',
                                allowEscapeKey: false,
                                allowOutsideClick: false,
                                didOpen: () => {
                                    Swal.showLoading()
                                }
                            });
                        }

                    }
                </script>
                <%- contentFor('BottomJs') %>