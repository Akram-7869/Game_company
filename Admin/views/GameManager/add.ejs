<%- contentFor('HeaderCss') %>
    <link href="/public/assets/libs/sweetalert2/sweetalert2.min.css" rel="stylesheet" type="text/css" />
    <link href="/public/assets/libs/dropzone/min/dropzone.min.css" rel="stylesheet" type="text/css" />

    <%- contentFor('breadcrumb') %>

        <!-- start page title -->
        <div class="row">
            <div class="col-12">
                <div class="page-title-box d-flex align-items-center justify-content-between">
                    <h4 class="mb-0">
                        <%- title %> Add
                    </h4>

                    <div class="page-title-right">
                        <ol class="breadcrumb m-0">
                            <li class="breadcrumb-item"><a href="javascript: void(0);">Home</a></li>
                            <li class="breadcrumb-item active"><a href="<%=process.env.ADMIN_URL%>/admin/bot">
                                    <%- title %> List
                                </a></li>
                            <li class="breadcrumb-item active">Add</li>
                        </ol>
                    </div>

                </div>
            </div>
        </div>
        <!-- end page title -->

        <%- contentFor('body') %>


            <div class="row">
                <div class="col-xl-12">
                    <div class="card">
                        <div class="card-body">
                            <!--  
                    <div class="mb-3">
                        <label class="form-label">Bot Complexity</label>
                        <input type="text" name="complexity" value="<%=row.complexity?row.complexity:'' %>" class="form-control" required placeholder="Bot Controle"/>
                    </div>-->
                            <form action="" enctype="multipart/form-data" method="POST" onsubmit="handleSubmit(this)">
                                <div class="mb-3">
                                    <label class="form-label">Name</label>
                                    <input type="text" id="name" name="name" value="<%=row.name?row.name:'' %>"
                                        class="form-control" required placeholder="name" />
                                </div>
                                <div class="mb-3">
                                    <label class="form-label">Version</label>
                                    <div class="col-md-10">
                                        <input type="text" id="version" name="version" value="<%=row.version?row.version:'' %>"
                                        class="form-control" required placeholder="version" />
                                    </div>
                                </div>
                                 
                                <div class="mb-3">
                                    <label class="form-label">Status</label>
                                    <div class="col-md-10">
                                        <select id="statustxt" class="form-select" name="status" required>
                                            <option value="active" <%=row.status==='active' ?'selected':'' %>>active
                                            </option>
                                            <option value="inactive" <%=row.status==='inactive' ?'selected':'' %>
                                                >inactive</option>
                                        </select>
                                    </div>
                                </div>

                                <div>
                                    <button id="submit-all" type="submit"
                                        class="btn btn-primary waves-effect waves-light me-1">
                                        Submit
                                    </button>

                                </div>

                            </form>

                        </div>
                    </div>
                </div> <!-- end col -->


            </div> <!-- end row -->

            <%- contentFor('FooterJs') %>
                <script src="/public/assets/libs/dropzone/min/dropzone.min.js"></script>

                <!-- parsleyjs -->
                <script src="/public/assets/libs/parsleyjs/parsley.min.js"></script>

                <script src="/public/assets/js/pages/form-validation.init.js"></script>
                <!-- Sweet Alerts js -->
                <script src="/public/assets/libs/sweetalert2/sweetalert2.min.js"></script>
                <!-- Sweet alert init js
<script src="public/assets/js/pages/sweet-alerts.init.js"></script>-->
                <script>
                    let url = '<%= apiUrl + "add" %>'
                    function handleSubmit(params) {
                        if (params.checkValidity()) {
                            //Swal.showLoading();
                            Swal.fire({
                                title: '',
                                html: 'Please wait...',
                                allowEscapeKey: false,
                                allowOutsideClick: false,
                                didOpen: () => {
                                    Swal.showLoading()
                                }
                            });
                        }

                    }
                  
                </script>
                <%- contentFor('BottomJs') %>