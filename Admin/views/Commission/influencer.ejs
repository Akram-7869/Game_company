                    src="/public/assets/libs/datatables.net-responsive-bs4/js/responsive.bootstrap4.min.js"></script>

                <!-- Datatable init js
<script src="/public/assets/js/pages/datatables.init.js"></script> -->
                <!-- Sweet Alerts js -->
                <script src="/public/assets/libs/sweetalert2/sweetalert2.min.js"></script>
                <script src="/public/assets/libs/moment/moment.js"></script>
                <%- contentFor('BottomJs') %>
                    <script>
                        var minDate, maxDate;
                        let day= new Date();
                        let nextDay = new Date(day);
                            nextDay.setDate(day.getDate() + 1);
                        $("#s_date").datepicker("setDate", day); 
                        $("#e_date").datepicker("setDate", nextDay); 
                        function delRec(id) {
                            let r = confirm('Want to pay ?');

                            if (r && id) {
                                handleDel(id);
                            }
                        }
                        function handleDel(id) {
                            Swal.showLoading();
                            return $.ajax({
                                type: 'POST',
                                url: '<%=process.env.ADMIN_URL%>/' + id,
                                success: (results) => {
                                    Swal.close();
                                    if (results.success) {
                                        alert(results.data.status + ' ' + results.data.message);

                                    } else {
                                        alert(results.error);

                                    }
                                    location.reload();
                                },
                                error: (err) => {
                                    alert('Error While Approving');
                                    location.reload();

                                }
                            });
                        }
                        function view(id) {
                            alert(id);
                        }
                        var i = 1;
                        var oTable = $('#listsetting').DataTable({
                            initComplete: function () {
                                var input = $('.dataTables_filter input').unbind(),
                                    self = this.api(),
                                    $searchButton = $('<button>')
                                        .text('search')
                                        .click(function () {
                                            // self.column(1).search('ccccccccccccccc').draw();
                                            self.search(input.val()).draw();
                                        }),
                                    $clearButton = $('<button>')
                                        .text('clear')
                                        .click(function () {
                                            input.val('');
                                            $searchButton.click();
                                        })
                                $('.dataTables_filter').append($searchButton, $clearButton);
                            },
                            "processing": true,
                            "serverSide": true,
                            "ajax": {
                                url: "<%=process.env.ADMIN_URL%>/influencer/commission/data", type: "post",
                                data: function (d) {
                                    d['s_date'] = $('#s_date').val();
                                    d['e_date'] = $('#e_date').val();
                                    // d['id'] =status-filter;
                                    d['paymentStatus'] = $('#status-filter').val();
                                    d['logType'] = $('#logtype-filter').val();
                                    return d;
                                }
                            },
                            "columns": [
                                {
                                    data: null, "render": function (data, type, full, meta) {
                                        return meta.row + 1;
                                    }
                                },
                                
                                { data: 'influencerCommission', "defaultContent": "",render: function (data) {
                                        return parseFloat(data).toFixed(2);
                                    } },
                                { data: 'totalGifts', "defaultContent": "",render: function (data) {
                                        return parseFloat(data).toFixed(2);
                                    } },

                                { data: 'logType', "defaultContent": "" },
                                { data: 'status', "defaultContent": "" },
                                {
                                    data: 'createdAt', render: function (data) {
                                        return moment(data).format('DD-MM-YYYY h:mm a');
                                    }
                                },
                                // {
                                //     data: null, "render": function (data, type, full, meta) {
                                //         let a = '<a class="px-2 text-primary" href="<%=process.env.ADMIN_URL%>/influencer/commission/edit/'
                                //             + full._id + '"><i class="uil uil-history font-size-18"></i></a>';

                                //         a += ' <a class="px-3 text-danger" onclick="delRec(\''
                                //             + full._id + '\')"><i class="uil uil-wallet font-size-18"></i></a> '

                                //         return a;
                                //     }
                                // },
                            ]
                        });
                      
                    </script>