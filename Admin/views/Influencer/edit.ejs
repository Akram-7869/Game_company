<%- contentFor('HeaderCss') %>
    <link href="/public/assets/libs/sweetalert2/sweetalert2.min.css" rel="stylesheet" type="text/css" />
    <%- contentFor('breadcrumb') %>

        <!-- start page title -->
        <div class="row">
            <div class="col-12">
                <div class="page-title-box d-flex align-items-center justify-content-between">
                    <h4 class="mb-0"><%- title %></h4>

                    <div class="page-title-right">
                        <ol class="breadcrumb m-0">
                            <li class="breadcrumb-item"><a href="javascript: void(0);">Home</a></li>
                            <li class="breadcrumb-item active">
                                <%- title %> List
                            </li>
                            <li class="breadcrumb-item active">Add</li>
                        </ol>
                    </div>

                </div>
            </div>
        </div>
        <!-- end page title -->

        <%- contentFor('body') %>


            <div class="row">
                <div class="col-xl-12">
                    <div class="card">
                        <div class="card-body">
                            <!--  <h4 class="card-title">Validation type</h4>
                <p class="card-title-desc"> </p> -->
                            <ul class="nav nav-tabs nav-tabs-custom nav-justified" role="tablist">
                                <li class="nav-item">
                                    <a class="nav-link active" data-bs-toggle="tab" href="#sitename" role="tab">
                                        <span class="d-block d-sm-none"><i class="fas fa-home"></i></span>
                                        <span class="d-none d-sm-block">Profile</span>
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link" data-bs-toggle="tab" href="#bank" role="tab">
                                        <span class="d-block d-sm-none"><i class="fas fa-cog"></i></span>
                                        <span class="d-none d-sm-block">Bank</span>
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link" data-bs-toggle="tab" href="#upi" role="tab">
                                        <span class="d-block d-sm-none"><i class="fas fa-cog"></i></span>
                                        <span class="d-none d-sm-block">UPI</span>
                                    </a>
                                </li>

                                <li class="nav-item">
                                    <a class="nav-link" data-bs-toggle="tab" href="#withdraw" role="tab">
                                        <span class="d-block d-sm-none"><i class="fas fa-cog"></i></span>
                                        <span class="d-none d-sm-block">Withdraw</span>
                                    </a>
                                </li>


                            </ul>
                            <div class="tab-content p-3 text-muted">

                                <div class="tab-pane active" id="sitename" role="tabpanel">
                                    <form class="custom-validation" action="" onsubmit="handleSubmit(this)"
                                        method="POST" name="player" enctype="application/x-www-form-urlencoded">
                                        <div class="mb-3">
                                            <label class="form-label">FirstName</label>
                                            <input type="text" name="firstName"
                                                value="<%=row.firstName?row.firstName:'' %>" class="form-control"
                                                required placeholder="First name" />
                                        </div>
                                        <div class="mb-3">
                                            <label class="form-label">LastName</label>
                                            <input type="text" name="lastName"
                                                value="<%=row.lastName?row.lastName:'' %>" class="form-control" required
                                                placeholder="Last name" />
                                        </div>
                                        <div class="mb-3">
                                            <label class="form-label">Display Name</label>
                                            <input type="text" name="displayName"
                                                value="<%=row.displayName?row.displayName:'' %>" class="form-control"
                                                required placeholder="Display Name " />
                                        </div>
                                        <div class="mb-3">
                                            <label class="form-label">Phone</label>
                                            <input type="text" name="phone" value="<%=row.phone?row.phone:'' %>"
                                                class="form-control" required placeholder="Phone" />
                                        </div>
                                        <div class="mb-3">
                                            <label class="form-label">Email</label>
                                            <input type="text" name="email" value="<%=row.email?row.email:'' %>"
                                                class="form-control" required placeholder="Email" />
                                        </div>
                                         
                                        <div class="mb-3">
                                            <label class="form-label">Status : <%=row.status%></label>
                                        </div>

                                        <div>
                                            <div>
                                                <button type="submit"
                                                    class="btn btn-primary waves-effect waves-light me-1">
                                                    Submit
                                                </button>
                                                <button type="reset" class="btn btn-secondary waves-effect">
                                                    Cancel
                                                </button>
                                            </div>
                                        </div>
                                    </form>


                                </div>


                                <div class="tab-pane" id="bank" role="tabpanel">
                                    <form id="bank" action="<%=process.env.ADMIN_URL%>/influencer/bank"
                                        onsubmit="handleSubmit(this)" enctype="multipart/form-data" method="POST">
                                        <div class="mb-3">
                                            <label class="form-label">Bank Account Holder Name</label>
                                           <input type="text" name="bankAccountHolder"
                                               value="<%=row.bankAccountHolder?row.bankAccountHolder:'' %>" class="form-control"
                                               required placeholder="Account Holder Name" />
                                       </div>
                                        <div class="mb-3">
                                             
                                            <label class="form-label">Bank Account Number</label>
                                            <input type="text" name="bankAccount"
                                                value="<%=row.bankAccount?row.bankAccount:'' %>" class="form-control"
                                                required placeholder="First name" />
                                        </div>
                                        <div class="mb-3">
                                            <label class="form-label">Bank IFC Code</label>
                                            <input type="text" name="bankIfc"
                                                value="<%=row.bankIfc?row.bankIfc:'' %>" class="form-control"
                                                required placeholder="First name" />
                                        </div>
                                        
                                            <div>
                                                <button type="submit" class="btn btn-primary waves-effect waves-light me-1">
                                                    Submit
                                                </button>
                                                <button type="reset" class="btn btn-secondary waves-effect">
                                                    Cancel
                                                </button>
                                            </div>
                                        
                                         
                                    </form>
                                </div>
                                <div class="tab-pane" id="upi" role="tabpanel">
                                    <form id="upi" action="<%=process.env.ADMIN_URL%>/influencer/upi"
                                        onsubmit="handleSubmit(this)" enctype="multipart/form-data" method="POST">
                                        <div class="mb-3">
                                            <label class="form-label">UPI ID</label>
                                           <input type="text" name="upiId"
                                               value="<%=row.upiId?row.upiId:'' %>" class="form-control"
                                               required placeholder="UPI ID" />
                                       </div>
                                       <div>
                                        <button type="submit" class="btn btn-primary waves-effect waves-light me-1">
                                            Submit
                                        </button>
                                        <button type="reset" class="btn btn-secondary waves-effect">
                                            Cancel
                                        </button>
                                    </div>
                                       
                                    </form>
                                </div>

                                <div class="tab-pane" id="withdraw" role="tabpanel">

                                    <form class="custom-validation" action="<%=process.env.ADMIN_URL%>/influencer/withdraw"
                                        onsubmit="handleSubmit(this)" method="POST" name="player"
                                        enctype="application/x-www-form-urlencoded">
    
                                        <div class="mb-3">
                                            <label class="form-label">Balance:<%= row.totalBalance%> </label>
    
                                        </div>
                                        <div class="mb-3">
                                            <label class="form-label">Amount</label>
                                            <input type="number" name="amount" value="" class="form-control"
                                                placeholder="Amount Withdraw" max="<%= row.totalBalance%> " />
                                        </div>
    
                                        <div>
                                            <div>
                                                <button type="submit" class="btn btn-primary waves-effect waves-light me-1">
                                                    Submit
                                                </button>
                                                <button type="reset" class="btn btn-secondary waves-effect">
                                                    Cancel
                                                </button>
                                            </div>
                                        </div>
                                    </form>
                                </div>

                            </div>


                        </div>


                    </div>
                </div>
            </div> <!-- end col -->


            </div> <!-- end row -->

            <%- contentFor('FooterJs') %>

                <!-- parsleyjs -->
                <script src="/public/assets/libs/parsleyjs/parsley.min.js"></script>

                <script src="/public/assets/js/pages/form-validation.init.js"></script>

                <script src="/public/assets/libs/sweetalert2/sweetalert2.min.js"></script>
                <!-- Sweet alert init js -->
                <script>
                    function handleSubmit(params) {
                        if (params.checkValidity()) {

                            Swal.showLoading();
                            //         Swal.fire({
                            //   title: '',
                            //   html: 'Please wait...',
                            //   allowEscapeKey: false,
                            //   allowOutsideClick: false,
                            //   didOpen: () => {
                            //     Swal.showLoading()
                            //   }
                            // });
                        }

                    }
                </script>
                <%- contentFor('BottomJs') %>